{
    "name": "adminetic/backup",
    "description": "Backup Panel For Adminetic Admin Panel",
    "keywords": [
        "adminetic",
        "backup"
    ],
    "homepage": "https://github.com/adminetic/backup",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "masbug/flysystem-google-drive-ext": "^2.2",
        "spatie/flysystem-dropbox": "^3.0",
        "spatie/laravel-backup": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adminetic\\Backup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adminetic\\Backup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adminetic\\Backup\\BackupServiceProvider"
            ],
            "aliases": {
                "Backup": "Adminetic\\Backup\\BackupFacade"
            }
        }
    }
}